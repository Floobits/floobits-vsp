<UserControl x:Class="Floobits.floobits_vsp.FlooTreeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d" 
             Name="FlooTreeWindow" d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label Name="Label" Visibility="Visible" Content="Floobits Unconnected" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TreeView Name="TreeView" Visibility="Hidden" HorizontalAlignment="Center" Height="280" Margin="10,10,10,10" VerticalAlignment="Center" Width="280"/>
    </Grid>
</UserControl>
