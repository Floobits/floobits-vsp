<ui:DialogWindow x:Class="Floobits.floobits_vsp.JoinWorkspaceURL"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ui="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.12.0"
        Title="Join Workspace by URL" Height="122" Width="400">
    <Grid>
        <TextBox Name="url" HorizontalAlignment="Left" Height="20" Margin="49,14,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="330"/>
        <Label Content="URL:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Target="{Binding ElementName=url}" RenderTransformOrigin="-0.676,0.5"/>
        <Button Content="Cancel" IsCancel="True" Click="Cancel_Click" HorizontalAlignment="Left" Margin="307,61,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Join" IsDefault="True" Click="Join_Click" HorizontalAlignment="Left" Margin="216,61,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</ui:DialogWindow>
